export io.qifan.server.ai.tag.root.entity.AiTag

input AiTagCreateInput {
    #allScalars(AiTag)
    id(aiFactory)
}
input AiTagUpdateInput {
    #allScalars(AiTag)
    id!
    id(aiFactory)
}

specification AiTagSpec {
    #allScalars
    like/i(service)
    like/i(id)
    ge(editedTime)
    le(editedTime)
    ge(createdTime)
    le(createdTime)
    id(creator)
    valueIn(id) as  ids
    id(aiFactory)
}